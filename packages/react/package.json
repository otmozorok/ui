{
  "name": "@otmozorok/react",
  "version": "0.39.0",
  "author": "Boris Hasikov",
  "description": "ui-kit из web components обернутый в react",
  "license": "MIT",
  "main": "./dist/cjs/index.js",
  "module": "./dist/es/index.js",
  "types": "./dist/types/index.d.ts",
  "source": "./src/index.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/es/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./*": "./*"
  },
  "sideEffects": [
    "*.css"
  ],
  "scripts": {
    "docs": "storybook dev -p 6003 --no-open",
    "build-docs": "storybook build",
    "build": "tsc -b && vite build -c vite.build.config.ts",
    "lint": "eslint ."
  },
  "peerDependencies": {
    "@types/react": ">=18",
    "@types/react-dom": ">=18",
    "react": ">=18",
    "react-dom": ">=18"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    },
    "@types/react-dom": {
      "optional": true
    }
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.46.0",
    "@vitejs/plugin-react": "^5.0.3",
    "@vitejs/plugin-react-swc": "^4.1.0",
    "eslint-plugin-react-hooks": "^7.0.0",
    "eslint-plugin-react-refresh": "^0.4.23",
    "typescript": "^5.2.2",
    "typescript-eslint": "^8.46.1",
    "vite": "^7.1.5"
  },
  "files": [
    "dist"
  ]
}
