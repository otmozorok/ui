{
  "name": "@otmozorok/react",
  "version": "0.20.1",
  "author": "Boris Hasikov",
  "description": "ui-kit из web components обернутый в react",
  "license": "MIT",
  "main": "./dist/cjs/index.js",
  "module": "./dist/es/index.js",
  "types": "./dist/types/index.d.ts",
  "source": "./src/index.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/es/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./*": "./*"
  },
  "sideEffects": [
    "*.css"
  ],
  "scripts": {
    "storybook": "storybook dev -p 6008 --no-open",
    "build-storybook": "storybook build",
    "build": "tsc -b && vite build -c vite.build.config.ts",
    "lint": "eslint ."
  },
  "peerDependencies": {
    "@types/react": ">=18",
    "@types/react-dom": ">=18",
    "react": ">=18",
    "react-dom": ">=18"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    },
    "@types/react-dom": {
      "optional": true
    }
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.43.0",
    "@vitejs/plugin-react": "^5.0.3",
    "@vitejs/plugin-react-swc": "^4.1.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "typescript": "^5.2.2",
    "vite": "^7.1.5"
  },
  "files": [
    "dist"
  ]
}
