{
  "name": "@otmozorok/preact",
  "version": "0.21.1",
  "author": "Boris Hasikov",
  "description": "ui-kit из web components обернутый в preact",
  "license": "MIT",
  "main": "./dist/cjs/index.js",
  "module": "./dist/es/index.js",
  "types": "./dist/types/index.d.ts",
  "source": "./src/index.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/es/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./*": "./*"
  },
  "sideEffects": [
    "*.css"
  ],
  "scripts": {
    "storybook": "storybook dev -p 6002 --no-open",
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview"
  },
  "peerDependencies": {
    "preact": ">=10"
  },
  "devDependencies": {
    "@preact/preset-vite": "^2.8.2",
    "typescript": "^5.2.2",
    "vite": "^7.1.4"
  },
  "files": [
    "dist"
  ]
}
