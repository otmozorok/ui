{
  "name": "@otmozorok/preact",
  "version": "0.46.0",
  "author": "Boris Hasikov",
  "description": "ui-kit из web components обернутый в preact",
  "license": "MIT",
  "main": "./dist/cjs/index.js",
  "module": "./dist/es/index.js",
  "types": "./dist/types/index.d.ts",
  "source": "./src/index.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/es/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./*": "./*"
  },
  "sideEffects": [
    "*.css"
  ],
  "scripts": {
    "docs": "storybook dev -p 6002 --no-open",
    "build-docs": "storybook build",
    "build": "tsc -b && vite build -c vite.build.config.ts",
    "lint": "eslint"
  },
  "peerDependencies": {
    "preact": ">=10"
  },
  "devDependencies": {
    "@preact/preset-vite": "^2.8.2"
  },
  "files": [
    "dist"
  ]
}
