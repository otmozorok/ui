{
  "name": "@otmozorok/svelte",
  "version": "0.20.6",
  "author": "Boris Hasikov",
  "description": "ui-kit из web components обернутый в svelte",
  "license": "MIT",
  "main": "./dist/cjs/index.js",
  "module": "./dist/es/index.js",
  "types": "./dist/types/index.d.ts",
  "source": "./src/index.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/es/index.js",
      "svelte": "./dist/es/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./*": "./*"
  },
  "sideEffects": [
    "*.css"
  ],
  "scripts": {
    "storybook": "storybook dev -p 6009 --no-open",
    "build": "tsc && vite build -c vite.build.config.ts",
    "check": "svelte-check --tsconfig ./tsconfig.build.json"
  },
  "peerDependencies": {
    "svelte": ">=5"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@tsconfig/svelte": "^5.0.2",
    "svelte-check": "^4.3.2",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2"
  },
  "files": [
    "dist"
  ]
}
