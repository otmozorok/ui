{
  "name": "@otmozorok/vue",
  "version": "0.40.0",
  "author": "Boris Hasikov",
  "description": "ui-kit из web components обернутый в vue",
  "license": "MIT",
  "main": "./dist/cjs/index.js",
  "module": "./dist/es/index.js",
  "types": "./dist/types/index.d.ts",
  "source": "./src/index.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/es/index.js",
      "vue": "./dist/es/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./*": "./*"
  },
  "sideEffects": [
    "*.css"
  ],
  "scripts": {
    "docs": "storybook dev -p 6005 --no-open",
    "build": "vue-tsc && vite build  -c vite.build.config.ts"
  },
  "peerDependencies": {
    "vue": ">=3"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^6.0.1",
    "typescript": "^5.2.2",
    "vite": "^7.1.4",
    "vue-tsc": "^3.1.1"
  },
  "files": [
    "dist"
  ]
}
