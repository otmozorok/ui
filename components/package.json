{
  "name": "@otmozorok/wc",
  "version": "0.34.0",
  "author": "Boris Hasikov",
  "description": "ui-kit из web components",
  "license": "MIT",
  "module": "./index.js",
  "types": "./types/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./index.js",
      "types": "./types/index.d.ts"
    },
    "./consts": {
      "import": "./consts/index.js",
      "types": "./types/consts/index.d.ts"
    },
    "./*": "./*"
  },
  "sideEffects": [
    "*.css"
  ],
  "homepage": "https://otmozorok.github.io/ui/",
  "repository": {
    "type": "git",
    "url": "https://github.com/otmozorok/ui.git"
  },
  "scripts": {
    "docs": "storybook dev -p 6001 --no-open",
    "build-docs": "storybook build",
    "lint": "eslint . --ext .js",
    "decl": "tsc index.js --allowJs -d --declarationDir types",
    "test": "vitest run -c vitest.dev.config.js"
  },
  "files": [
    "consts",
    "lib",
    "props",
    "types",
    "utils",
    "web-component",
    "style.css",
    "index.js",
    "!lib/**/stories.*",
    "!lib/**/spec.*"
  ]
}
